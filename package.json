{
  "name": "vscode-tree-extension",
  "displayName": "Tree",
  "description": "Lauguage support for Tree files",
  "version": "0.0.2",
  "publisher": "CTC",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Hacksore/vscode-tree-language.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "tree",
        "extensions": [
          ".tree"
        ],
        "aliases": [
          "tree",
          "tree-file"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tree",
        "scopeName": "source.tree",
        "path": "./syntaxes/tree.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "package": "vsce package"
  },
  "devDependencies": {
    "vsce": "^1.95.1"
  }
}
