{
  "name": "vscode-tree-extension",
  "displayName": "Tree",
  "description": "Lauguage support for Tree files",
  "version": "0.0.4",
  "publisher": "CTC",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.28.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:extension.tree.reload",
    "onLanguage:tree",
    "onStartupFinished"
  ],
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/structure-codes/vscode-tree-language.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.tree.reload",
        "title": "Tree: reload"
      }
    ],
    "languages": [
      {
        "id": "tree",
        "extensions": [
          ".tree"
        ],
        "aliases": [
          "tree",
          "tree-file"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tree",
        "scopeName": "source.tree",
        "path": "./syntaxes/tree.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "lint": "eslint . --ext .ts,.tsx",
    "build": "tsc",
    "dev": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/vscode": "^1.28.0",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "typescript": "^4.4.4",
    "vsce": "^1.95.1"
  }
}
